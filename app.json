{
  "expo": {
    "name": "RentOne - Gestion Locative",
    "slug": "rentone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "rentone",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2563eb"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rentone.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "RentOne a besoin d'accéder à vos photos pour ajouter des images de vos biens.",
        "NSCameraUsageDescription": "RentOne a besoin d'accéder à votre caméra pour prendre des photos de vos biens.",
        "NSContactsUsageDescription": "RentOne a besoin d'accéder à vos contacts pour faciliter l'ajout de locataires."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.rentone.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2563eb"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.rentone.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "rentone-production"
      }
    },
    "updates": {
      "enabled": false
    },
    "owner": "kameltalbi",
    "privacy": "public",
    "description": "Application professionnelle de gestion locative pour propriétaires. Gérez vos biens, locataires, paiements et dépenses en toute simplicité.",
    "primaryColor": "#2563eb"
  }
}
